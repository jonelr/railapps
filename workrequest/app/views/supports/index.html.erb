<h1>Listing supports</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Workrequest</th>
    <th>Category</th>
    <th>Assignedto</th>
    <th>Status</th>
    <th>Plant</th>
    <th>Businessunit</th>
    <th>Queue</th>
    <th>Description</th>
    <th>Created by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @supports.each do |support| %>
  <tr>
    <td><%= support.title %></td>
    <td><%= support.workrequest_id %></td>
    <td><%= support.category_id %></td>
    <td><%= support.assignedto_id %></td>
    <td><%= support.status_id %></td>
    <td><%= support.plant_id %></td>
    <td><%= support.businessunit_id %></td>
    <td><%= support.queue %></td>
    <td><%= support.description %></td>
    <td><%= support.created_by %></td>
    <td><%= link_to 'Show', support %></td>
    <td><%= link_to 'Edit', edit_support_path(support) %></td>
    <td><%= link_to 'Destroy', support, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Support', new_support_path %>
